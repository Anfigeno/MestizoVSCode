        "variable.language.this",
        "markup.deleted"
      ],
      "settings": {
        "foreground": "#ff5263",
        "fontStyle": "normal"
      }
    },
    {
      "name": "Verde",
      "scope": [
        "string",
        "text.html.php string",
        "markup.inline.raw",
        "markup.inserted",
        "punctuation.definition.string",
